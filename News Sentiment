timent.vader import SentimentIntensityAnalyzer\n","\n","from sklearn.feature_extraction.text import CountVectorizer, TfidfTransformer\n","from sklearn.feature_selection import SelectKBest, mutual_info_classif\n","from sklearn.naive_bayes import MultinomialNB\n","from sklearn.pipeline import Pipeline\n","from sklearn.model_selection import train_test_split\n","from sklearn.metrics import classification_report\n","\n","import matplotlib.pyplot as plt\n","import spacy\n","nlp = spacy.load(\"en_core_web_sm\")  # for entity extraction (optional)\n","\n","# ---------- Helper functions ----------\n","STOP = set(stopwords.words('english'))\n","lemmatizer = WordNetLemmatizer()\n","\n","def simple_clean(text):\n","    \"\"\"Lower + remove extra whitespace.\"\"\"\n","    text = text.replace('\\n',' ').strip()\n","    text = re.sub(r'\\s+', ' ', text)\n","    return text\n","\n","def tokenize_and_lemmatize(sentence):\n","    tokens = word_tokenize(sentence)\n","    # keep punctuation tokens for negation boundary detection\n","    tokens_clean = []\n","    for t in tokens:\n","        if re.match(r'^\\W+$', t):  # punctuation token\n","            tokens_clean.append(t)\n","        else:\n","            w = t.lower()\n","            if w not in STOP and re.search(r'[A-Za-z0-9]', w):\n","                lw = lemmatizer.lemmatize(w)\n","                tokens_clean.append(lw)\n","            else:\n","                # keep punctuation in tokens_clean to help negation detection\n","                if re.match(r'^\\W+$', t):\n","                    tokens_clean.append(t)\n","    return tokens_clean\n","\n","def negation_mark(tokens):\n","    \"\"\"\n","    Simple negation handling: after a negator, mark words until punctuation as NEG.\n","    Example: \"not good at all\" -> [\"not\", \"good_NEG\",\"at_NEG\",\"all_NEG\"]\n","    \"\"\"\n","    neg_terms = set(['not','no','never',\"n't\",'nor','cannot','neither'])\n","    out = []\n","    neg = False\n","    for t in tokens:\n","        tl = t.lower()\n","        if re.match(r'^[\\.\\!\\?\\,\\;\\:]+$', t):\n","            neg = False\n","            out.append(t)\n","            continue\n","        if tl in neg_terms:\n","            neg = True\n","            out.append(t)\n","            continue\n","        if neg:\n","            out.append(t + '_NEG')\n","        else:\n","            out.append(t)\n","    return out\n","\n","def sentence_preprocessor(sentence):\n","    # 1) simple clean\n","    s = simple_clean(sentence)\n","    # 2) tokenization\n","    tokens = word_tokenize(s)\n","    # 3) negation marking (do before removing punctuation)\n","    tokens_marked = negation_mark(tokens)\n","    # 4) remove punctuation tokens and stopwords + lemmatize\n","    tokens_final = []\n","    for t in tokens_marked:\n","        if re.match(r'^[\\.\\!\\?\\,\\;\\:]+$', t):\n","            continue\n","        w = t.lower()\n","        if w.endswith('_neg'):  # keep neg-marked tokens\n","            base = w[:-4]\n","            if re.search(r'[A-Za-z0-9]', base) and base not in STOP:\n","                tokens_final.append(base + '_NEG')\n","            continue\n","        if w not in STOP and re.search(r'[A-Za-z0-9]', w):\n","            tokens_final.append(lemmatizer.lemmatize(w))\n","    return \" \".join(tokens_final)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":495},"id":"5NpU3SylmMxn","executionInfo":{"status":"error","timestamp":1757878113942,"user_tz":-345,"elapsed":7594,"user":{"displayName":"Bajagain Binod","userId":"07549480069871011892"}},"outputId":"ab5705bc-a56d-4a0e-da0f-e58bf24f7cd3"},"execution_count":null,"outputs":[{"output_type":"error","ename":"KeyboardInterrupt","evalue":"","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mAttributeError\u001b[0m                            Traceback (most recent call last)","\u001b[0;32m/usr/lib/python3.12/pathlib.py\u001b[0m in \u001b[0;36m__st